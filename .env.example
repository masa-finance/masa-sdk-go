# Base URL for the Masa Protocol API (default: http://localhost:8080)
MASA_PROTOCOL_BASE_URL=http://localhost:8080

# API path prefix for all endpoints (default: /api/v1/data)
MASA_PROTOCOL_API_PATH=/api/v1/data

# Nineteen AI Configuration
NINETEEN_API_KEY=your_api_key_here  # Required: Get your API key from https://nineteen.ai
NINETEEN_BASE_URL=https://api.nineteen.ai/v1  # Optional: Override default base URL
NINETEEN_CHAT_COMPLETIONS_PATH=/chat/completions  # Optional: Override default chat completions path

# Logging configuration
LOG_LEVEL=DEBUG  # Available: DEBUG, INFO, WARN, ERROR
